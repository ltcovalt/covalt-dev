{
	"extends": "astro/tsconfigs/base",
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist"],
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@components": ["src/components/index.js"],
			"@components/*": ["src/components/*"],
			"@layouts": ["src/layouts/index.js"],
			"@layouts/*": ["src/layouts/*"],
			"@utils": ["src/utils/index.js"],
			"@utils/*": ["src/utils/*"]
		},
		"allowJs": true,
		"checkJs": false,
		"noImplicitAny": false,
		"noEmit": true
	}
}
