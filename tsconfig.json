{
	"extends": "astro/tsconfigs/strict",
	"include": [
		".astro/types.d.ts",
		"**/*"
	],
	"exclude": [
		"dist"
	],
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@components": [ "src/components/index"],
			"@components/*": [ "src/components/*" ],
			"@layouts": ["src/layouts/index"],
			"@layouts/*": [ "src/layouts/*" ],
			"@pages": [ "src/pages/index"],
			"@pages/*": [ "src/pages/*" ]
		}
	}
}